{
  "pyprland": {
    "plugins": [
      "scratchpads"
    ]
  },
  "scratchpads": {
    "term": {
      "command": "alacritty --class alacritty-dropterm",
      "animation": "fromTop",
      "unfocus": "hide"
    },
    "ranger": {
        "command": "kitty --class explorer -e ranger",
        "animation": "fromBottom",
        "unfocus": "hide"
    },
    "file-manager": {
      "command": "thunar",
      "unfocus": "hide",
      "animation": "fromBottom"
    }
  }
}
